@model IEnumerable<Contact>
<div class="untree_co-hero overlay" style="background-image: url('images/img-school-1-min.jpg');">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-12">
                <div class="row justify-content-center ">
                    <div class="col-lg-6 text-center ">
                        <h1 class="mb-4 heading text-white" data-aos="fade-up" data-aos-delay="100">Branches</h1>



                    </div>


                </div>

            </div>

        </div> <!-- /.row -->
    </div> <!-- /.container -->

</div> <!-- /.untree_co-hero -->


<div class="untree_co-section">
    <div class="container">
        <div class="row">
            @foreach(var contact in Model)
           {
            <div class="col-6 col-sm-6 col-md-6 col-lg-4" data-aos="fade-up" data-aos-delay="100">
                <div class="feature">
                     <h3>@contact.BranchName</h3>
                    <p>@contact.Address</p>
                    <small>@contact.PhoneNumber</small>
                </div>
            </div>
           }
            
        </div>
    </div> <!-- /.container -->
</div> <!-- /.untree_co-section -->
